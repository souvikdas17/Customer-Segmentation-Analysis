    "    sse.append(km.inertia_)"
    "plt.figure(figsize=(12,6))\n",
    "plt.plot(range(1,11),sse)\n",
    "plt.plot(range(1,11),sse, linewidth=2, color=\"red\")\n",
    "plt.xlabel(\"K Value\")\n",
    "plt.xticks(np.arange(1,11,1))\n",
    "plt.ylabel(\"SSE\")\n",
    "plt.show()"
    "km1=KMeans(n_clusters=5)\n",
    "km1.fit(df2)\n",
    "y=km1.predict(df2)\n",
    "df['label']=y"
